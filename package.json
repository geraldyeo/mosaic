{
  "author": "Canva",
  "name": "mosaic",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint src --ext .js",
    "test": "nyc ava | faucet",
    "posttest": "nyc report",
    "build": "webpack --progress --colors",
    "prestart": "npm run build",
    "start": "node server.js"
  },
  "ava": {
    "files": [
      "./src/**/*.test.js"
    ],
    "failFast": true,
    "tap": true,
    "verbose": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "dependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "webpack": "1.12.15"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "electron-prebuilt": "0.37.5",
    "eslint": "2.7.0",
    "eslint-config-xo": "0.13.0",
    "eslint-loader": "1.3.0",
    "faucet": "0.0.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "karma": "0.13.22",
    "karma-coverage": "0.5.5",
    "karma-electron-launcher": "0.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-tap": "1.0.4",
    "karma-tap-reporter": "0.0.6",
    "karma-webpack": "1.7.0",
    "nyc": "6.4.0",
    "tape": "4.5.1"
  }
}
